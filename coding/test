#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int a ,b ,*p,*q;
	p=&a;
	q=&b;
	a=1;
	b=2;
	p=(int*)malloc(a*sizeof(int));
    q=(int*)malloc(a*sizeof(int));
	*p=1;
	*q=2;
	printf("%d     ",p);
	printf("%d,",q);
	return 0;
}
